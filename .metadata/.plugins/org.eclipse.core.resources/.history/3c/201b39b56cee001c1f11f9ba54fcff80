		}
		
		for (int i= 0; i < mulheres.length; i++) {
			System.out.printf("Digite a idade da mulher %d:", (i+1));
			mulheres[i] = input.nextInt();
		}
		
		input.close();
		
		for (int i = 0; i < homens.length; i++) {
			
		}
		
		System.out.println();
	}	
}
